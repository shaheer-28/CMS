<%= form_for [:admin, @camp] do |f| %>
  <table id="show_user_table">
    <tr>
      <th><%= f.label "ID" %></th>
      <td><%= @camp.id %></td>
    </tr>
    <tr>
      <th><%= f.label "Location" %></th>
      <td><%= f.text_field :location, autofocus: true, required: true %> </td>
    </tr>
  </table>

  <%= f.submit "Update", disable_with: 'Updating...' %>
<% end %>

<%= link_to "Back", admin_camps_path %>
