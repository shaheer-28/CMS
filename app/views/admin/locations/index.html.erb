<%= link_to "Users Listing", admin_users_path %><br>
<%= link_to "Add New Camp Location", new_admin_location_path %>
<table>
  <tr>
    <td>
      <%= form_with method: :get, local: true do |f| %>
        <%= f.text_field :search_key, value: params[:search_key] %>
        <%= f.submit 'search'%>
      <% end %>
    </td>
  </tr>
  <tr>
    <th><%= sortable "ID" %></th>
    <th><%= sortable "LOCATION" %></th>
    <th colspan="3"></th>
  </tr>
  <%= render @locations %>
</table>
<%== pagy_nav(@pagy).html_safe %>
<%= link_to "Sign out", destroy_user_session_path, method: :delete %>
