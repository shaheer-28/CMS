<table>
  <tr>
    <th>Name</th>
    <th>Locations</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Status</th>
  </tr>
  <tr>
    <td><%= @camp.name %></td>
    <td>
      <ul>
        <% @camp.locations.map do |l| %>
          <li><%= l.location %></li>
        <% end %>
      </ul>
    </td>
    <td><%= @camp.start_date %></td>
    <td><%= @camp.end_date %></td>
    <td><%= @camp.status %></td>
  </tr>
</table>
<%= link_to "Sign out", destroy_user_session_path, method: :delete %>
